{
  "name": "haitaton",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "serve": "serve -s build -l 3000",
    "build-and-serve": "yarn run build && yarn run serve",
    "test": "react-scripts test --env=jest-environment-jsdom-sixteen",
    "eject": "react-scripts eject",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write",
    "ts-check": "tsc --noEmit --project tsconfig.json",
    "ts-check:cypress": "tsc --noEmit --project ./cypress/tsconfig.json",
    "lint": "yarn ts-check:cypress && yarn ts-check && eslint --ext js,ts,tsx src cypress",
    "lint:css": "stylelint 'src/**/*.scss'",
    "e2e": "start-server-and-test start :3000 cypress-run-chrome",
    "cypress-run-chrome": "cypress run --browser chrome",
    "cypress-run-chrome-headless": "cypress run --headless --browser chrome"
  },
  "dependencies": {
    "@chakra-ui/core": "^0.8.0",
    "@emotion/core": "^10.0.35",
    "@emotion/styled": "^10.0.27",
    "@reduxjs/toolkit": "^1.4.0",
    "@types/geojson": "^7946.0.7",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/ol": "^6.4.1",
    "@types/react": "^16.9.0",
    "@types/react-datepicker": "^3.1.1",
    "@types/react-dom": "^16.9.0",
    "@types/react-helmet": "^6.1.0",
    "@types/react-redux": "^7.1.9",
    "@types/react-router-dom": "^5.1.5",
    "@types/styled-components": "^5.1.3",
    "@types/turf": "^3.5.32",
    "axios": "^0.20.0",
    "clsx": "^1.1.1",
    "date-fns": "^2.16.1",
    "emotion-theming": "^10.0.27",
    "eslint-config-airbnb-typescript-prettier": "^3.1.0",
    "geojson": "^0.5.0",
    "hds-design-tokens": "^0.14.0",
    "hds-react": "^0.14.0",
    "i18next": "^19.8.2",
    "msw": "^0.21.2",
    "node-sass": "^4.14.1",
    "ol": "^6.4.3",
    "react": "^16.13.1",
    "react-datepicker": "^3.3.0",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^6.9.2",
    "react-i18next": "^11.7.3",
    "react-query": "^2.21.0",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.0-next.98",
    "redux-thunk": "^2.3.0",
    "serve": "^11.3.2",
    "ts-enum-util": "^4.0.2",
    "turf": "^3.0.14",
    "typescript": "4.0.2"
  },
  "devDependencies": {
    "@testing-library/dom": "^7.24.2",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.4",
    "@testing-library/user-event": "^12.1.5",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "cypress": "^5.3.0",
    "eslint-config-airbnb-typescript": "^10.0.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-cypress": "^2.11.2",
    "husky": "^4.3.0",
    "jest-environment-jsdom": "^26.0.1",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "lint-staged": "^10.3.0",
    "prettier": "^2.1.1",
    "prettier-eslint": "^11.0.0",
    "start-server-and-test": "^1.11.5",
    "stylelint": "^13.7.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-value-no-unknown-custom-properties": "^3.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn run type-check"
    }
  },
  "lint-staged": {
    "*.@(js|jsx|ts|tsx)": [
      "yarn lint"
    ],
    "*.@(css|scss)": [
      "yarn lint:css"
    ],
    "*.@(js|jsx|ts|tsx|scss|css|md)": [
      "yarn format"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "resolutions": {
    "**/react-scripts/**/@typescript-eslint/eslint-plugin": "^4.4.1",
    "**/react-scripts/**/@typescript-eslint/parser": "^4.4.1"
  },
  "jest": {
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!ol)/"
    ]
  }
}
